package exam01;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String str = scanner.nextLine();
		
		char n; 
		int m ;
		int result = 0;
		
		for(int i = 0; i<3 ; i++) {
			n= str.charAt(i);
			m= (int) n;
			result += m;
		}
	}
}
